<div class="chat-bubble-wrapper">
 <% sender = message.user == current_user ? "me" : "her" %>
 <div class="chat-bubble <%= sender %>">
   <div class="chat-username"><%= message.user.identifier %></div>
   <div class="chat-message"><%= message.content %></div>
 </div>

 <div class="chat-timestamp <%= sender %>"><%= message.created_at.strftime("%m-%e-%Y %H:%M") %></div>
</div>
